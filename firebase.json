{
  "hosting": {
    
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "asia-east1"
    },
    "rewrites": [
      {
        "destination": "/postPage/[postId]/index.html",
        "regex": "^/postPage/([^/]+?)(?:/)?$"
      }
    ]
  }
}
